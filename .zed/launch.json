{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Main with UV",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/Contabilidad IConstruye/main.py",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/Contabilidad IConstruye/.venv/bin/python",
      "cwd": "${workspaceFolder}/Contabilidad IConstruye",
      "env": {},
      "envFile": "${workspaceFolder}/Contabilidad IConstruye/.env"
    },
    {
      "name": "Debug with UV Command",
      "type": "python",
      "request": "launch",
      "program": "uv",
      "args": ["run", "python", "main.py"],
      "console": "integratedTerminal",
      "cwd": "${workspaceFolder}/Contabilidad IConstruye",
      "env": {},
      "envFile": "${workspaceFolder}/Contabilidad IConstruye/.env"
    },
    {
      "name": "Debug Specific Module with UV",
      "type": "python",
      "request": "launch",
      "module": "src.services.scraper",
      "console": "integratedTerminal",
      "python": "${workspaceFolder}/Contabilidad IConstruye/.venv/bin/python",
      "cwd": "${workspaceFolder}/Contabilidad IConstruye",
      "env": {}
    }
  ]
}
